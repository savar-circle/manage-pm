{
    "builds": [{
            "src": "projectpm/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "projectpm/wsgi.py"
        }
    ]
}